<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.Badgujar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Launcher">
        <activity android:name="com.example.android.Badgujar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.android.Badgujar.NewsActivity"/>
        <activity android:name="com.example.android.Badgujar.MatrimonialActivity"/>
        <activity android:name="com.example.android.Badgujar.DirectoryActivity"/>
        <activity android:name="com.example.android.Badgujar.BusinessActivity"/>
        <activity android:name="com.example.android.Badgujar.SamajMandalActivity"/>
        <activity android:name="com.example.android.Badgujar.HelpingHandActivity"/>
        <activity android:name="com.example.android.Badgujar.HistoryActivity"/>
        <activity android:name="com.example.android.Badgujar.AboutusActivity"/>
        <activity android:name="com.example.android.Badgujar.ContactusActivity"/>
    </application>

</manifest>